#include <iostream>
using namespace std;
int main() {
    int n, m, k;
    k = 0;
    int arr[500][500];
    cin >> n >> m;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
                arr[i][j] = i * 2 + j;
        }
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
}
